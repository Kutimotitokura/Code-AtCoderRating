{
  "name": "code-atcoderrating",
  "displayName": "Code-AtCoderRating",
  "description": "",
  "version": "0.0.1",
  "publisher": "kutimotitokura",
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ],
    "configuration": [
      {
        "title": "AtCoderRating",
        "properties": {
          "atcoderrating.username": {
            "type": "string",
            "default": "tourist",
            "description": "your AtCoder username"
          }
        }
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "cheerio-httpcli": "^0.7.3"
  }
}
